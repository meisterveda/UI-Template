{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/angular/angular-tailwind-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "packages/angular/angular-tailwind-e2e/cypress.json",
        "devServerTarget": "angular-angular-tailwind:serve:development"
      },
      "configurations": {
        "production": {
          "devServerTarget": "angular-angular-tailwind:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/angular/angular-tailwind-e2e/**/*.{js,ts}"
        ]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["angular-angular-tailwind"]
}
