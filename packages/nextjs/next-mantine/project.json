{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/nextjs/next-mantine",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "packages/nextjs/next-mantine",
        "outputPath": "dist/packages/nextjs/next-mantine"
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "nextjs-next-mantine:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "nextjs-next-mantine:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "nextjs-next-mantine:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "nextjs-next-mantine:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/nextjs/next-mantine"],
      "options": {
        "jestConfig": "packages/nextjs/next-mantine/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/nextjs/next-mantine/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": []
}
